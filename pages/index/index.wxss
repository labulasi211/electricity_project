page {
  height: 100%;
}
.electricity_project SearchInput {
  background-color: var(--themeColor);
}
.electricity_project .index_container {
  height: calc(100vh - 80rpx);
}
.electricity_project .index_container .index_swiper {
  width: 750rpx;
  height: 340rpx;
  padding: var(--themeUpMargin) var(--themeRightMargin);
  background: url(https://img.imgdb.cn/item/60226a6c3ffa7d37b32ec150.png);
  background-size: 100%;
}
.electricity_project .index_container .index_swiper .swiper_position {
  display: flex;
  height: 100%;
  width: 100%;
  border-radius: var(--themeRadius);
}
.electricity_project .index_container .index_swiper .swiper_position swiper {
  height: 100%;
  width: 100%;
  border-radius: 35rpx;
  overflow: hidden;
  transform: translateY(0);
}
.electricity_project .index_container .index_swiper .swiper_position swiper navigator .swiper-image {
  width: 100%;
  height: 100%;
}
.electricity_project .index_container .index_cate {
  display: flex;
}
.electricity_project .index_container .index_cate navigator {
  flex: 1;
  padding: var(--themeUpMargin) var(--themeRightMargin);
}
.electricity_project .index_container .index_cate navigator image {
  width: 100%;
}
.electricity_project .index_container .index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.electricity_project .index_container .index_floor .floor_group .floor_title image {
  width: 100%;
}
.electricity_project .index_container .index_floor .floor_group .floor_list {
  overflow: hidden;
}
.electricity_project .index_container .index_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
  border-radius: var(--themeRadius);
  overflow: hidden;
}
.electricity_project .index_container .index_floor .floor_group .floor_list navigator:nth-last-child(-n+4) {
  height: calc(33.33vw * 386 / 232 / 2);
  border-left: 10rpx solid #fff;
}
.electricity_project .index_container .index_floor .floor_group .floor_list navigator:nth-child(2),
.electricity_project .index_container .index_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.electricity_project .index_container .index_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
